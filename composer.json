{
  "name": "twigwp/twig-wordpress",
  "type": "library",
  "description": "Twig for WordPress as Service",
  "keywords": [
    "wordpress",
    "twig",
    "psr",
    "composer"
  ],
  "homepage": "https://github.com/widoz/twig-wordpress",
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Guido Scialfa",
      "email": "dev@guidoscialfa.com",
      "homepage": "http://www.guidoscialfa.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/widoz/twig-wordpress"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "TwigWp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TwigWp\\Tests\\": "tests/php",
      "TwigWp\\Tests\\Unit\\": "tests/php/unit/src",
      "TwigWp\\Tests\\Integration\\": "tests/php/integration/src"
    }
  },
  "require": {
    "php": ">=7.0",
    "twig/twig": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "brain/monkey": "dev-master",
    "inpsyde/php-coding-standards": "^0.13.3"
  }
}
