{
  "name": "unprefix/unprefix-twig-wordpress",
  "type": "wordpress-theme",
  "description": "Twig for WordPress as Service",
  "keywords": [
    "wordpress",
    "twig",
    "psr",
    "composer"
  ],
  "homepage": "http://www.unprefix.com",
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Guido Scialfa",
      "email": "dev@guidoscialfa.com",
      "homepage": "http://www.guidoscialfa.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/widoz/unprefix-twig-wordpress"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Unprefix\\Twig\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Unprefix\\Twig\\Tests\\": "tests/php",
      "Unprefix\\Twig\\Tests\\Unit\\": "tests/php/unit/src",
      "Unprefix\\Twig\\Tests\\Integration\\": "tests/php/integration/src"
    }
  },
  "optimize-autoloader": true,
  "classmap-authoritative": true,
  "require": {
    "php": ">=7.0",
    "twig/twig": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5.5",
    "brain/monkey": "dev-master",
    "automattic/phpcs-neutron-standard": "~1.2.0",
    "wp-coding-standards/wpcs": "~0.14.0",
    "object-calisthenics/phpcs-calisthenics-rules": "~2.0.0"
  }
}
